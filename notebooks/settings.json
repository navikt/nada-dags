  },

  "db_connection_strings": {
    "datalab": "${DATALAB_CONNECTION_STRING}",
    "postgres": "${POSTGRES_CON_STRING}"
  },
    
  "db_vault_path": {
    "postgres": "${POSTGRES_CREDS_PATH}"
  },
    
  "kafka": {
